{
  "name": "app",
  "version": "14.34.0",
  "author": "lenne.Tech <info@lenne.tech> (https://lenne.tech)",
  "license": "MIT",
  "scripts": {
    "init": "npm install",
    "ng": "ng",
    "reinit": "rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force && npm i",
    "start": "ng serve",
    "start:external": "ng serve --host 0.0.0.0",
    "build": "ng build --configuration production",
    "build:local": "ng build && ng run app:server:development",
    "build:develop": "ng build && ng run app:server:develop",
    "build:test": "ng build --configuration test",
    "test": "ng test --watch=false --browsers ChromeHeadless",
    "test:watch": "ng test --browsers ChromeHeadless",
    "test:browser": "ng test",
    "lint": "ng lint",
    "watch": "ng build --watch --configuration development",
    "format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --write",
    "format:check": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --check",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "update": "node extras/sync-packages.mjs",
    "dev:ssr": "ng run app:serve-ssr",
    "serve:ssr": "node dist/app/server/main.js",
    "build:ssr": "ng build && ng run app:server",
    "prerender": "ng run app:prerender",
    "link:ng-base": "yalc add @lenne.tech/ng-base && yalc link @lenne.tech/ng-base && npm install",
    "unlink:ng-base": "yalc remove @lenne.tech/ng-base && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.12",
    "@angular/cdk": "14.2.7",
    "@angular/common": "14.2.12",
    "@angular/compiler": "14.2.12",
    "@angular/core": "14.2.12",
    "@angular/forms": "14.2.12",
    "@angular/platform-browser": "14.2.12",
    "@angular/platform-browser-dynamic": "14.2.12",
    "@angular/platform-server": "14.2.12",
    "@angular/router": "14.2.12",
    "@apollo/client": "3.7.8",
    "@lenne.tech/ng-base": "14.35.0",
    "@ng-bootstrap/ng-bootstrap": "13.1.1",
    "@popperjs/core": "2.11.6",
    "@nguniversal/express-engine": "14.2.3",
    "apollo-angular": "4.2.1",
    "bootstrap": "5.2.3",
    "bootstrap-icons": "1.10.3",
    "compression": "1.7.4",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-ws": "5.11.3",
    "ngx-image-cropper": "6.3.2",
    "localstorage-polyfill": "1.0.1",
    "rxjs": "7.5.7",
    "tslib": "2.4.1",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.10",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "14.2.10",
    "@angular/compiler-cli": "14.2.12",
    "@angular/localize": "14.2.12",
    "@nguniversal/builders": "14.2.3",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.17",
    "@types/jasmine": "4.3.1",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "jasmine-core": "4.5.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "prettier": "2.8.4",
    "prettier-eslint": "15.0.1",
    "standard-version": "9.5.0",
    "typescript": "4.8.4"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  }
}
